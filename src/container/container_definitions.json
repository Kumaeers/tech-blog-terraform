  
[
  {
    "name": "${name}",
    "image": "${account_id}.dkr.ecr.${region}.amazonaws.com/${name}:${tag}",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "${region}",
        "awslogs-group": "/ecs/${name}",
        "awslogs-stream-prefix": "vue"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 80
      }
    ]
  }
]
